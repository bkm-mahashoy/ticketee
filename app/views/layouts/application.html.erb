<!DOCTYPE html>
<html>
<head>
  <title><%= @title || Ticketee %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% flash.each do |key, value| %>
  <div class="flash" id="<%= key %>">
    <%= value %>
  </div>
<% end %>

<h3><%= link_to "Ticketee", root_path %></h3>
<%= yield %>

</body>
</html>
